<view class="container"><view wx:if="{{a}}"><picker mode="date" bindchange="{{c}}"><view class="date-picker"><text>{{b}}</text></view></picker><view wx:for="{{d}}" wx:for-item="item" wx:key="f" class="inquiry-item"><view class="item-info"><text class="item-name">{{item.a}}</text><text class="item-price">价格: ¥{{item.b}}</text></view><view class="item-edit"><input class="item-input" type="number" placeholder="修改售卖价格" value="{{item.c}}" bindinput="{{item.d}}"/><text class="item-date">{{item.e}}</text></view></view><button class="submit-button" bindtap="{{e}}">提交审核</button></view><view wx:else><text class="no-permission">您没有权限查看询价单</text></view></view>